    </servlet>

    <servlet-mapping>
        <servlet-name>userServlet</servlet-name>
        <url-pattern>/untitled_war_exploded</url-pattern>
    </servlet-mapping>


        <servlet-mapping>
            <servlet-name>Servletdel</servlet-name>
            <url-pattern>/user/del</url-pattern>
        </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Servlet2 </servlet-name>
        <url-pattern>/user/find</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
