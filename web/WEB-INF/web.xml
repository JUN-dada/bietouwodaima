    <servlet>
        <servlet-name>userServlet</servlet-name>
        <servlet-class>com.zijun.contor.userServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet2</servlet-name>
        <servlet-class>com.zijun.contor.Servlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servletdel</servlet-name>
